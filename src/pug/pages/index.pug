						.features__text Відкритість до інновацій
	.principles(data-section="six" id="principles")
		.container
			.principles__icon
				+svg('nav-logo')
			.principles__title.title Our principles
			ul.principles__list
				li.principles__item
					.principles__img
						+img('principles-img.png')
					.principles__small-title Тільки генеральне планування
					.principles__desc Наші об’єкти – це живі автономні організми, навколо яких ми створюємо та розвиваємо інфраструктуру: дороги, школи, зони відпочинку, пляжі.
				li.principles__item
					.principles__img
						+img('principles-img2.png')
					.principles__small-title Найвищі стандарти якості
					.principles__desc Адже ми будуємо для тих, хто звик отримувати найкраще.
				li.principles__item
					.principles__img
						+img('principles-img3.png')
					.principles__small-title Турбота має початок, але не має кінця
					.principles__desc Наша участь у проекті не завершується на етапі забудови. Ми здійснюємо якісне сервісне обслуговування житлових комплексів, продовжуємо покращувати територію поблизу.
				li.principles__item
					.principles__img
						+img('principles-img4.png')
					.principles__small-title Антропоцентризм
					.principles__desc Ми будуємо не стіни та дах, а особливий спосіб життя для людей нового формату мислення. Ми створюємо базу для ком’юніті, де воля, демократичний підхід та самоуправління всебічно підтримані на рівні комфорту та сервісу.

	.partners(data-section="seven" id="partners")
		.container
			.partners__top
				.partners__title.title Our partners
				.partners__slider-controls.slider-controls
					button(type="button" class="slider-controls__left slider-controls__arrow js-partners-left")
						+svg('short-arrow')
					.slider-controls__decor
					button(type="button" class="slider-controls__right slider-controls__arrow js-partners-right btn--res")
						+svg('short-arrow')
			.swiper-container.js-partners__slider.partners__slider-wrap
				.swiper-wrapper.partners__slider
					.partners__slide-item.swiper-slide
						.partners__slide.partners__slide--1
							+img('ui/partners-img.svg')
						.partners__slide.partners__slide--5
							+img('ui/partners-img5.svg')
					.partners__slide-item.swiper-slide
						.partners__slide.partners__slide--2
							+img('ui/partners-img2.svg')
						.partners__slide.partners__slide--6
							+img('ui/partners-img6.svg')
